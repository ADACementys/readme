{
	"name": "@appnest/readme",
	"version": "1.0.0",
	"description": "A readme generator",
	"license": "MIT",
	"scripts": {
		"b:lib": "node build.js",
		"readme": "npm run b:lib && node dist/index.cjs.js"
	},
	"contributors": [
		{
			"name": "Andreas Mehlsen",
			"email": "andmehlsen@gmail.com",
			"url": "https://twitter.com/andreasmehlsen"
		}
	],
	"bin": {
		"readme": "index.cjs.js"
	},
	"dependencies": {
		"colors": "^1.3.3",
		"fs-extra": "^7.0.1",
		"minimist": "^1.2.0",
		"path": "^0.12.7"
	},
	"devDependencies": {
		"rimraf": "^2.6.3",
		"rollup": "^1.1.2"
	},
	"readme": {
		"logo": {
			"url": "https://avatars1.githubusercontent.com/u/6267397?s=460&v=4",
			"width": 500
		},
		"text": "Use this readme generator to easily generate pretty readme's like this one! Simply extend your <code>package.json</code>.",
		"input": "readme/blueprint.md",
		"output": "README.md",
		"ids": {
			"github": "andreasbm/lit-translate",
			"npm": "@appnest/lit-translate",
			"webcomponents": "@appnest/lit-translate"
		},
		"badges": [
			{
				"text": "Awesome",
				"url": "https://github.com/web-padawan/awesome-lit-html",
				"img": "https://awesome.re/badge.svg"
			}
		],
		"bullets": [
			"Bullet 1",
			"Bullet 2",
			"Bullet 3"
		]
	}
}
